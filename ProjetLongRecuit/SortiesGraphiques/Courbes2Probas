vect=200*(1:length(u0));
taille = length(u0);
umean=zeros(taille,1);
uintervalleincertitude = zeros(taille,2);
for i = 1:taille;
			umean(i) = mean(u(:,i));
			nombredeVecteurs=length(u(:,1));
			uincertitude(i) = std(u(:,i))/sqrt(nombredeVecteurs);
			uintervalleincertitude(i,1) = umean(i) + uincertitude(i)*1.96;
			uintervalleincertitude(i,2) = umean(i) - uincertitude(i)*1.96;
			end;
