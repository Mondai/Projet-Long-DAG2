vect=1000*(1:length(u0));
taille = length(u0);
umean=zeros(taille,1);
uintervalleincertitude = zeros(taille,2);
for i = 1:taille;
			umean(i) = mean(u(:,i));
			nombredeVecteurs=length(u(:,1));
			uincertitude(i) = std(u(:,i))/sqrt(nombredeVecteurs);
			uintervalleincertitude(i,1) = umean(i) + uincertitude(i)*1.96;
			uintervalleincertitude(i,2) = umean(i) - uincertitude(i)*1.96;
			end;
plot(vect,uintervalleincertitude,'k-');
hold on;
title('data/dsjc250.5.col')
xlabel('Nombre d iterations');
ylabel('Energie');
legend({['Recuit Simulé Linéaire, k=1.0 up'], ['Recuit Simulé Linéaire, k=1.0 down']});